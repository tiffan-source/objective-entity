{
  "name": "objective-entity",
  "version": "1.0.0",
  "description": "A simple TypeScript template for Node.js projects",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiffan-source/my-ts-startup.git"
  },
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "prepare": "husky",
    "build": "npx tsc",
    "changeset": "npx changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "typescript",
    "node",
    "template",
    "starter"
  ],
  "author": "blackgenius",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "todo-entity": "^2.4.0"
  }
}
